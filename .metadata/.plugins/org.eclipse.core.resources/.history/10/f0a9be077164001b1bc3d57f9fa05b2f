
public class MainTrenes {

	public static void main(String[] args) {

		Estacion estacion = new Estacion();
		
		for (int i = 0; i < 10; i++) {
			
			Tren tren = new Tren(estacion,i+1);
			tren.start();
			try {
				tren.join();
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}

		
	}

}
