
public class Orden extends Thread{
	
	private ControlarPrimero con;
	private int numero;
	
	Orden(ControlarPrimero c, int entero){
		
		con = c;
		numero = entero;
		
	}

	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		super.run();
		
		if(numero==2) {
			
			System.out.println("Numero 2");
		
		}
		else {
			System.out.println("Numero 1");
		}
		
		
	}
	
}
